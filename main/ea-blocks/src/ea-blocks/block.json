{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/ea-blocks",
	"version": "0.1.0",
	"title": "Ea Blocks",
	"category": "widgets",
	"icon": "calendar",
	"description": "Booking Appointments",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"width": {
			"type": "string",
			"default": "400px"
		},
		"scrollOff": {
			"type": "boolean",
			"default": false
		},
		"layoutCols": {
			"type": "string",
			"default": "1"
		},
		"location": {
			"type": "string",
			"default": ""
		},
		"service": {
			"type": "string",
			"default": ""
		},
		"worker": {
			"type": "string",
			"default": ""
		},
		"defaultDate": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "ea-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}